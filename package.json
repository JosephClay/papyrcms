{
  "name": "nextjs-cms",
  "version": "1.0.0",
  "description": "A dynamic CMS template made with Next.js",
  "engines": {
    "node": "9.11.2"
  },
  "main": "index.js",
  "scripts": {
    "deev": "nodemon server.js",
    "dev": "NODE_ENV=local nodemon server.js",
    "deploy": "npm run heroku-config && git push heroku master",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "heroku-config": "heroku config:set $(cat .env | sed '/^$/d; /#[[:print:]]*$/d')",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.17.0",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "google-map-react": "^1.1.5",
    "googleapis": "^45.0.0",
    "handlebars": "^4.5.3",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.7.13",
    "multer": "^1.4.2",
    "next": "^9.1.4",
    "nodemailer": "^6.4.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "react": "^16.12.0",
    "react-ckeditor-component": "^1.1.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-redux": "^7.1.3",
    "react-render-html": "^0.6.0",
    "react-stripe-elements": "^6.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sanitize-html": "^1.20.1",
    "stripe": "^7.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1"
  }
}
